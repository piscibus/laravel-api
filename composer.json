{
    "name": "piscibus/laravel-api",
    "description": "Yet another Laravel api package.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Piscibus\\LaravelApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "nunomaduro/collision": "^5.4",
        "orchestra/testbench": "^6.17"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Piscibus\\LaravelApi\\LaravelApiServiceProvider"
            ]
        }
    }
}
